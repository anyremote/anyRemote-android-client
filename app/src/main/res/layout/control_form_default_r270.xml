                    android:scaleType="centerInside" >
                </ImageButton>
            </LinearLayout>
        </TableRow>

        <TableRow
            android:id="@+id/row3_r270"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/tl_b12_r270" >

                <ImageButton
                    android:id="@+id/b12_r270"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:background="@drawable/btn_selector"
                    android:contentDescription="@string/b12"
                    android:nextFocusRight="@+id/b9_r270"
                    android:nextFocusUp="@+id/b11_r270"
                    android:scaleType="centerInside" >
                </ImageButton>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/tl_b9_r270" >

                <ImageButton
                    android:id="@+id/b9_r270"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:background="@drawable/btn_selector"
                    android:contentDescription="@string/b9"
                    android:nextFocusLeft="@+id/b6_r270"
                    android:nextFocusRight="@+id/b12_r270"
                    android:nextFocusUp="@+id/b8_r270"
                    android:scaleType="centerInside" >
                </ImageButton>
            </LinearLayout>
            
          <LinearLayout
                android:id="@+id/tl_b6_r270" >

                <ImageButton
                    android:id="@+id/b6_r270"
                    android:layout_width="wrap_content"
