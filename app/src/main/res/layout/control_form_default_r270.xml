                    android:contentDescription="@string/b6"
                    android:nextFocusLeft="@+id/b9_r270"
                    android:nextFocusRight="@+id/b3_r270"
                    android:nextFocusUp="@+id/b5_r270"
                    android:scaleType="centerInside" >
                </ImageButton>
            </LinearLayout>
            
            <LinearLayout
                android:id="@+id/tl_b3_r270" >

                <ImageButton
                    android:id="@+id/b3_r270"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
